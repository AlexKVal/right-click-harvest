{
  "schema_version": 1,
  "mixin": [
  ],
  "quilt_loader": {
    "group": "io.github.jamalam360",
    "id": "rightclickharvest",
    "version": "${version}",
    "metadata": {
      "name": "Right Click Harvest",
      "description": "Right click crops to harvest them - with style.",
      "contributors": {
        "Jamalam": "Owner"
      },
      "contact": {
        "sources": "https://github.com/JamCoreModding/right-click-harvest"
      },
      "icon": "assets/rightclickharvest/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "io.github.jamalam360.rightclickharvest.quilt.RightClickHarvestQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=${minecraft_version}"
      },
      {
        "id": "architectury",
        "version": ">=${architectury_version}"
      },
      {
        "id": "jamlib",
        "version": ">=${jamlib_version}"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}
